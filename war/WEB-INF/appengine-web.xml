<appengine-web-app xmlns='http://appengine.google.com/ns/1.0'>
  <application>ssp-preview</application>
  <version>1-0-3</version>
  <threadsafe>true</threadsafe>
  <ssl-enabled>false</ssl-enabled>
  <system-properties>
    <property name='ssp.cookie.enc' value='@{encKey}' />
    <property name='aws.access.key' value='' />
    <property name='aws.secret.access.key' value='' />
  </system-properties>

  <static-error-handlers>
    <handler file="/errors/default_error.html" />
    <handler file="/errors/over_quota.html" error-code="over_quota" />
  </static-error-handlers>
</appengine-web-app>
